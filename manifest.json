{
  "manifest_version": 3,
  "name": "Dia Home",
  "version": "1.0.0",
  "description": "A bookmarks home page for Dia browser",
  "permissions": [
    "bookmarks",
    "storage",
    "favicon",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
